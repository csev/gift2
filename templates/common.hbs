<div id="question{{count}}">
  <h1>{{__ 'Question'}} {{count}}</h1>
  <input type="button" value="X" name="btn_remove_question{{count}}"
    onclick='$("#question{{count}}").remove(); renumber_questions();'>
  <label for="title_question{{count}}">{{__ 'Question Title:'}}</label>
  <input type="text" name="title_question{{count}}" value="{{name}}">
  <br>
  <label for="text_question{{count}}">{{__ 'Question Text:'}}</label>
  <input type="text" name="text_question{{count}}" value="{{question}}">
  <input type="hidden" name="type_question{{count}}" value="{{type}}">
  <div id="content_question{{count}}"></div>
</div>
